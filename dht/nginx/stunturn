server {
    listen 443 ssl;
    server_name stun.nasiadka.pl, turn.nasiadka.pl;

    location / {
        proxy_pass http://localhost:3478;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
        proxy_set_header X-Forwarded-Proto $scheme;
    }

	include /etc/nginx/includes/headers;
    include /etc/nginx/includes/errors;
    include /etc/nginx/includes/bots403;
    include /etc/nginx/includes/geoblock;
}